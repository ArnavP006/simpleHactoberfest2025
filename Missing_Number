#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Enter n (max number): ";
    cin >> n;

    vector<int> arr(n - 1);
    cout << "Enter " << n - 1 << " numbers:\n";
    int sum = 0;
    for (int i = 0; i < n - 1; i++) {
        cin >> arr[i];
        sum += arr[i];
    }

    int total = n * (n + 1) / 2;
    int missing = total - sum;

    cout << "Missing number is: " << missing << endl;
    return 0;
}
